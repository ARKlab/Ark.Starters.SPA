{
  "name": "ark.starters.spa",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "sideEffects": [
    "cypress/**/*",
    "src/index.tsx",
    "src/initGlobals.tsx"
  ],
  "scripts": {
    "start": "concurrently --kill-others --success first \"npm:start:*\"",
    "start:connectionStrings": "cross-env NODE_ENV=development PORT=4000 node --watch -- public/connectionStrings.cjs",
    "prestart:app": "wait-on --log --timeout 30000 http-get://localhost:4000",
    "start:app": "vite",
    "start:chakratheme": "npx @chakra-ui/cli typegen --watch --strict src/theme.ts",
    "prebuild": "npx @chakra-ui/cli typegen --clean --strict src/theme.ts",
    "build": "vite build",
    "analyze": "vite build --mode analyze",
    "prepreview": "cross-env npm run build",
    "preview": "concurrently --kill-others --success first \"npm:preview:*\"",
    "preview:connectionStrings": "cross-env PORT=4000 node -- public/connectionStrings.cjs",
    "prepreview:app": "wait-on --log --timeout 30000 http-get://localhost:4000",
    "preview:app": "cross-env PORT=4321 vite preview",
    "lint": "concurrently \"npm:lint:*\"",
    "lint:oxlint": "oxlint --type-aware --type-check",
    "lint:typecheck:app": "tsgo --project ./tsconfig.json --noEmit",
    "lint:typecheck:cypress": "tsgo --project ./cypress/tsconfig.json --noEmit",
    "format": "oxfmt",
    "format:check": "oxfmt --check",
    "test": "concurrently --kill-others --success first \"npm:e2e:ci:*\"",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "e2e:start": "cross-env BROWSER=none VITE_MODE=e2e concurrently --kill-others \"npm:start\" \"npm:cypress:open\"",
    "e2e:ci:connectionStrings": "cross-env VITE_MODE=e2e PORT=4000 node -- public/connectionStrings.cjs",
    "pree2e:ci:app": "cross-env-shell NODE_ENV=production CYPRESS_COVERAGE=true \"vite build --mode e2e\"",
    "e2e:ci:app": "cross-env NODE_ENV=production BROWSER=none vite preview --mode e2e",
    "pree2e:ci:run": "wait-on --log --timeout 360000 http-get://localhost:4000 http-get://localhost:3000",
    "e2e:ci:run": "cross-env NODE_ENV=production npm run cypress:run",
    "e2e:dev": "cross-env BROWSER=none VITE_MODE=e2e concurrently --kill-others \"npm:start\" \"npm:cypress:run\""
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "2.15.0",
    "@azure/msal-browser": "5.2.0",
    "@chakra-ui/react": "3.33.0",
    "@dnd-kit/core": "6.3.1",
    "@dnd-kit/sortable": "10.0.0",
    "@dnd-kit/utilities": "3.2.2",
    "@emotion/react": "11.14.0",
    "@hookform/resolvers": "5.2.2",
    "@microsoft/applicationinsights-clickanalytics-js": "3.3.11",
    "@microsoft/applicationinsights-react-js": "19.3.8",
    "@microsoft/applicationinsights-web": "3.3.11",
    "@reduxjs/toolkit": "2.11.2",
    "@semihbou/zod-i18n-map": "0.0.3",
    "@tanstack/react-table": "8.21.3",
    "@tinyhttp/content-disposition": "2.2.4",
    "date-fns": "4.1.0",
    "error-stack-parser": "2.1.4",
    "fast-deep-equal": "3.1.3",
    "i18next": "25.8.5",
    "js-cookie": "3.0.5",
    "next-themes": "0.4.6",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-error-boundary": "6.1.0",
    "react-hook-form": "7.71.1",
    "react-i18next": "16.5.4",
    "react-icons": "5.5.0",
    "react-redux": "9.2.0",
    "react-router": "7.13.0",
    "stacktrace-js": "2.0.2",
    "usehooks-ts": "3.1.1",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@chakra-ui/cli": "3.33.0",
    "@cypress/code-coverage": "4.0.0",
    "@iodigital/vite-plugin-msw": "2.0.0",
    "@svgr/plugin-jsx": "8.1.0",
    "@svgr/plugin-svgo": "8.1.0",
    "@testing-library/cypress": "10.1.0",
