  "scripts": {
    "start": "concurrently --kill-others --success first \"npm:start:*\"",
    "start:connectionStrings": "cross-env NODE_ENV=development PORT=4000 node --watch -- public/connectionStrings.cjs",
    "prestart:app": "wait-on --log --timeout 30000 http-get://localhost:4000",
    "start:app": "vite",
    "start:chakratheme": "npx @chakra-ui/cli typegen --watch --strict src/theme.ts",
    "prebuild": "npx @chakra-ui/cli typegen --clean --strict src/theme.ts",
    "build": "vite build",
    "prepreview": "cross-env npm run build",
    "preview": "concurrently --kill-others --success first \"npm:preview:*\"",
    "preview:connectionStrings": "cross-env PORT=4000 node -- public/connectionStrings.cjs",
    "prepreview:app": "wait-on --log --timeout 30000 http-get://localhost:4000",
    "preview:app": "cross-env PORT=4321 vite preview",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "e2e:start": "cross-env BROWSER=none VITE_MODE=e2e concurrently --kill-others \"npm:start\" \"npm:cypress:open\"",
    "e2e:ci": "concurrently --kill-others --success first \"npm:e2e:ci:*\"",
    "e2e:ci:connectionStrings": "cross-env PORT=4000 node -- public/connectionStrings.cjs",
    "pree2e:ci:app": "cross-env NODE_ENV=production CYPRESS_COVERAGE=true vite build --mode e2e",
    "e2e:ci:app": "cross-env NODE_ENV=production BROWSER=none vite preview --mode e2e",
    "pree2e:ci:run": "wait-on --log --timeout 120000 http-get://localhost:4000 http-get://localhost:3000",
    "e2e:ci:run": "cross-env NODE_ENV=production npm run cypress:run"
  },
